{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@4.0.1",
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/scan", "dest": "/api/main.py" },
    { "src": "/api/health", "dest": "/api/main.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
